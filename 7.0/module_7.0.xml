<?xml version="1.0" encoding="UTF-8"?>
<project name="module_7.0" default="compile.module.7.0">
  <dirname property="module.7.0.basedir" file="${ant.file.module_7.0}"/>
  
  <property name="module.jdk.home.7.0" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.7.0" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.7.0" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.7.0" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="7.0.output.dir" value="${module.7.0.basedir}/out/production/7.0"/>
  <property name="7.0.testoutput.dir" value="${module.7.0.basedir}/out/test/7.0"/>
  
  <path id="7.0.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="7.0.module.production.classpath">
    <path refid="${module.jdk.classpath.7.0}"/>
    <path refid="library.json-simple-1.1.1.classpath"/>
  </path>
  
  <path id="7.0.runtime.production.module.classpath">
    <pathelement location="${7.0.output.dir}"/>
    <path refid="library.json-simple-1.1.1.classpath"/>
  </path>
  
  <path id="7.0.module.classpath">
    <path refid="${module.jdk.classpath.7.0}"/>
    <pathelement location="${7.0.output.dir}"/>
    <path refid="library.json-simple-1.1.1.classpath"/>
  </path>
  
  <path id="7.0.runtime.module.classpath">
    <pathelement location="${7.0.testoutput.dir}"/>
    <pathelement location="${7.0.output.dir}"/>
    <path refid="library.json-simple-1.1.1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.7.0">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.7.0">
    <patternset refid="excluded.from.module.7.0"/>
  </patternset>
  
  <path id="7.0.module.sourcepath">
    <dirset dir="${module.7.0.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.7.0" depends="compile.module.7.0.production,compile.module.7.0.tests" description="Compile module 7.0"/>
  
  <target name="compile.module.7.0.production" depends="register.custom.compilers" description="Compile module 7.0; production classes">
    <mkdir dir="${7.0.output.dir}"/>
    <javac2 destdir="${7.0.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.7.0}/javac">
      <compilerarg line="${compiler.args.7.0}"/>
      <bootclasspath refid="7.0.module.bootclasspath"/>
      <classpath refid="7.0.module.production.classpath"/>
      <src refid="7.0.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.7.0"/>
    </javac2>
    
    <copy todir="${7.0.output.dir}">
      <fileset dir="${module.7.0.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.7.0.tests" depends="register.custom.compilers,compile.module.7.0.production" description="compile module 7.0; test classes" unless="skip.tests"/>
  
  <target name="clean.module.7.0" description="cleanup module">
    <delete dir="${7.0.output.dir}"/>
    <delete dir="${7.0.testoutput.dir}"/>
  </target>
</project>